{
  "name": "@mvp-react/react-datagrid-mui",
  "version": "0.1.3",
  "description": "A light datagrid build upon react-table-plain for React in a Material-UI theme.",
  "main": "lib/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "npm-run-all -s lint transpile",
    "transpile": "tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "test": "jest --runInBand --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "db": "json-server --watch db/db.json -p 5000",
    "isready": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvp-react/react-components.git"
  },
  "keywords": [
    "npm",
    "node",
    "react",
    "table",
    "sorting",
    "filter",
    "paging",
    "javascrip",
    "typescript"
  ],
  "author": "Stephan Mei√üner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvp-react/react-components/issues"
  },
  "homepage": "https://github.com/mvp-react/react-components/packages/react-datagrid-mui#readme",
  "dependencies": {
    "@mvp-react/react-table-plain": "workspace:*",
    "@mvp-react/react-table-mui": "workspace:*"
  },
  "peerDependencies": {
    "@material-ui/core": "4.4.2",
    "@material-ui/icons": "4.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@mvp-react/react-datagrid-plain": "workspace:*",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/node-logger": "^6.0.22",
    "@storybook/react": "^5.1.10",
    "@types/jest": "^26.0.3",
    "@types/node": "^12.12.2",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "7.10.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-formatjs": "^2.3.13",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "prettier": "2.0.5",
    "prettier-eslint": "10.1.1",
    "prettier-eslint-cli": "5.0.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^3.9.6",
    "webpack": "^4.44.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "c**/?(*.)(spec|test).ts?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}