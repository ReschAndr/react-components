{
  "name": "@mvp-react/react-datagrid-plain",
  "version": "0.1.3",
  "description": "A light datagrid build upon react-table-plain for React.",
  "main": "lib/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "run-s lint test transpile",
    "transpile": "tsc",
    "lint": "tslint ./src/**/*.ts*",
    "test": "jest --runInBand --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvp-react/react-components.git"
  },
  "keywords": [
    "npm",
    "node",
    "react",
    "table",
    "sorting",
    "filter",
    "paging",
    "javascrip",
    "typescript"
  ],
  "author": "Stephan Mei√üner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvp-react/react-components/packages/issues"
  },
  "homepage": "https://github.com/mvp-react/react-components/packages/react-datagrid-plain#readme",
  "dependencies": { "@mvp-react/react-table-plain": "workspace:*"},
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@mvp-react/react-table-mui": "workspace:*",
    "@mvp-react/react-table-plain": "workspace:*",
    "@mvp-react/react-table-semantic-ui": "workspace:*",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@storybook/node-logger":"^6.0.22",
    "@types/jest": "^26.0.3",
    "@types/node":"^12.12.2",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.13.1",
    "regenerator-runtime":"^0.13.7",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.6",
    "webpack":"^4.44.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "c**/?(*.)(spec|test).ts?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
