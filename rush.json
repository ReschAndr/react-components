{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.34.4",
  "pnpmVersion": "5.11.0",
  "pnpmOptions": {
    "strictPeerDependencies": false,
    "resolutionStrategy": "fast",
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "gitPolicy": {
    "versionBumpCommitMessage": "Applying package updates. [skip-ci]"
  },
  "repository": {
    "url": "https://github.com/mvp-react/react-components.git",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "eslint-config-mvp-react",
      "projectFolder": "packages/eslint-config-mvp-react",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "documentation-app",
      "projectFolder": "documentation/app",
      "reviewCategory": "production",
      "shouldPublish": false
    },
    {
      "packageName": "@mvp-react/react-table-plain",
      "projectFolder": "packages/react-table-plain",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@mvp-react/react-table-mui",
      "projectFolder": "packages/react-table-mui",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@mvp-react/react-table-semantic-ui",
      "projectFolder": "packages/react-table-semantic-ui",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@mvp-react/react-datagrid-plain",
      "projectFolder": "packages/react-datagrid-plain",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@mvp-react/react-datagrid-mui",
      "projectFolder": "packages/react-datagrid-mui",
      "reviewCategory": "production",
      "shouldPublish": true
    }
  ]
}